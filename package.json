{
	"name": "elixir-utils",
	"displayName": "Elixir Utils",
	"description": "Add support command for Elixir development",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.40.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/koga1020/vscode-elixir-utils.git"
	},
	"publisher": "koga1020",
	"license": "MIT",
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:extension.createTestFile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createTestFile",
				"title": "Elixir Utils: create test file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"@types/mkdirp": "^0.5.2",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
